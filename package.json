{
    "name": "lineblame",
    "displayName": "LineBlame",
    "description": "Bring Git blame annotations to line of file in VS Code",
    "version": "0.0.6",
    "homepage": "https://github.com/baliscript/vscode-lineblame/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/baliscript/vscode-lineblame.git"
    },
    "license": "MIT",
    "publisher": "baliscript",
    "engines": {
        "vscode": "^1.30.0"
    },
    "main": "./src/extension.js",
    "icon": "icons/lineblame.png",
    "activationEvents": [
        "*"
    ],
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "LineBlame",
            "properties": {
                "lineblame.dateFormat": {
                    "type": "string",
                    "default": "YYYY/MM/DD",
                    "enum": [
                        "YYYY/MM/DD",
                        "MM/DD/YYYY",
                        "DD/MM/YYYY"
                    ]
                }
            }
        }
    },
    "categories": [
        "Other"
    ]
}
